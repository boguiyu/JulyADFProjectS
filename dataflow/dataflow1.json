{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SourceData",
						"type": "DatasetReference"
					},
					"name": "Purchasing"
				},
				{
					"dataset": {
						"referenceName": "SourceData",
						"type": "DatasetReference"
					},
					"name": "Application"
				},
				{
					"dataset": {
						"referenceName": "SourceData",
						"type": "DatasetReference"
					},
					"name": "Warehouse"
				},
				{
					"dataset": {
						"referenceName": "SourceData",
						"type": "DatasetReference"
					},
					"name": "Sales"
				}
			],
			"sinks": [
				{
					"name": "sink1"
				},
				{
					"name": "sink2"
				},
				{
					"name": "sink3"
				},
				{
					"name": "sink4"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey1"
				},
				{
					"name": "surrogateKey2"
				},
				{
					"name": "surrogateKey3"
				},
				{
					"name": "surrogateKey4"
				},
				{
					"name": "WaterMark"
				},
				{
					"name": "WaterMark1"
				},
				{
					"name": "WaterMark2"
				},
				{
					"name": "WaterMark3"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select *\\nfrom information_schema.TABLES\\nwhere Table_Schema = \\'Purchasing\\'',",
				"     format: 'query') ~> Purchasing",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select *\\nfrom information_schema.TABLES\\nwhere Table_Schema = \\'Application\\'',",
				"     format: 'query') ~> Application",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select *\\nfrom information_schema.TABLES\\nwhere Table_Schema = \\'Warehouse\\'',",
				"     format: 'query') ~> Warehouse",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select *\\nfrom information_schema.TABLES\\nwhere Table_Schema = \\'Sales\\'',",
				"     format: 'query') ~> Sales",
				"Purchasing keyGenerate(output(SurrogateKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"Application keyGenerate(output(SurrogateKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey2",
				"Warehouse keyGenerate(output(SurrogateKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey3",
				"Sales keyGenerate(output(SurrogateKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey4",
				"surrogateKey1 derive(Watermakr = currentDate()) ~> WaterMark",
				"surrogateKey2 derive(WaterMark = currentDate()) ~> WaterMark1",
				"surrogateKey3 derive(WaterMark = ERROR_FUNCTION('')) ~> WaterMark2",
				"surrogateKey4 derive(WaterMark = currentDate()) ~> WaterMark3",
				"WaterMark sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1",
				"WaterMark1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink2",
				"WaterMark2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink3",
				"WaterMark3 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink4"
			]
		}
	}
}